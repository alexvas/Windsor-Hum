<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
    >
  <ui:style>
    .menu {
      overflow: hidden;
    }
  </ui:style>
  <g:HTMLPanel styleName="{style.menu}">
    <g:MenuBar animationEnabled="true">
      <g:MenuItem text="Edit">
        <g:MenuBar vertical="true" animationEnabled="true">
          <g:MenuItem ui:field="reportNewHum" text="Report A New Hum Case"/>
          <g:MenuItem ui:field="editLastHum" text="Edit The Last Hum Case"/>
        </g:MenuBar>
      </g:MenuItem>
      <g:MenuItem text="Analyse">
        <g:MenuBar vertical="true" animationEnabled="true">
          <g:MenuItem ui:field="overview" text="Show Me The Latest Reports"/>
          <g:MenuItem ui:field="yesterday" text="Show Me Reports From Yesterday"/>
          <g:MenuItem ui:field="lastDecade" text="Show Me Reports In The Last 10 Days"/>
        </g:MenuBar>
      </g:MenuItem>
    </g:MenuBar>
  </g:HTMLPanel>
</ui:UiBinder>
