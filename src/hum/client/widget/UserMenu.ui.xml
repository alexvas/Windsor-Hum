      <g:MenuItem ui:field="addNewHum" text="Add New Entry"/>
      <g:MenuItem ui:field="editLastHum" text="Edit Last Entry"/>
      <g:MenuItem ui:field="overview" text="Overview"/>
    </g:MenuBar>
  </g:HTMLPanel>
</ui:UiBinder>
